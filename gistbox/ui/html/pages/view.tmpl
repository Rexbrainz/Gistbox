{{define "title"}}Gist #{{.Gist.ID}}{{end}}

{{define "main"}}
  {{with .Gist}}
  <div class='snippet'>
    <div class='metadata'>
      <strong>{{.Title}}</strong>
      <span>#{{.ID}}</span>
    </div>
    <pre><code>{{.Content}}</code></pre>
    <div class='metadata'>
      <time>Cretead: {{humanDate .Created}}</time>
      <time>Expries: {{humanDate .Expires}}</time>
    </div>
  </div>
  {{end}}
{{end}}
